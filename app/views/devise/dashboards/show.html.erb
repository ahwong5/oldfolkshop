<h1> User Profile </h1>

<table>
  <thead>
    <tr>
      <th>User ID</th>
      <th>Name</th>
      <th>email</th>
      <th>Address</th>
      <th>Phone</th>
      <th>image</th>
      <th>role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.user_id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.address %></td>
        <td><%= user.phone %></td>
        <td><%= user.image %></td>
        <td><%= user.role %></td>
      </tr>
    <% end %>
  </tbody>
</table>
