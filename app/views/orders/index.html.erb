<h1> All Orders </h1>

<table>
  <thead>
    <tr>
      <th>Order ID</th>
      <th>User ID</th>
      <th>Total Price</th>
      <th>Status</th>
      <th>Bill ID</th>
      <th>Due At</th>
      <th>Paid At</th>
      <th>Paid Date Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.id %></td>
        <td><%= order.user_id %></td>
        <td><%= order.totalprice %></td>
        <td><%= order.status %></td>
        <td><%= order.bill_id %></td>
        <td><%= order.due_at %></td>
        <td><%= order.paid_at %></td>
        <td><%= order.paiddatetime %></td>
      </tr>
    <% end %>
  </tbody>
</table>
